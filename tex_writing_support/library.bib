issn = {08856087},
journal = {Hydrological Processes},
keywords = {ARTIFICIAL NEURAL-NETWORKS,CLASSIFICATION,DIFFERENTIAL EVOLUTION,EFFICIENT,FRAMEWORK,GLOBAL OPTIMIZATION,HYDROLOGIC-MODELS,IMPROVED CALIBRATION,REGIONAL FLOOD FREQUENCY,UNCERTAINTY,accepted 18 march 2010,calibration,catchment modeling,clustering,diagnostic evaluation,received 2 june 2009},
month = sep,
