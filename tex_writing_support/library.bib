@phdthesis{VanHoey2008,
abstract = {Onvolledige of foutieve neerslaggegevens zijn een belangrijke oorzaak van onzekerheid in het hydrologisch modelleren van een stroomgebied, voornamelijk in regio's met sterke ruimtelijke variabiliteit zoals in de tropen. Fouten in metingen van neerslag zijn onvermijdelijk en de slechte ruimtelijke resolutie van de neerslagstations in deze gebieden maakt het extra moeilijk om deze variabiliteit correct op te meten. In (semi)ruimtelijk verdeelde modellen is een correct begrip van de ruimtelijke variabiliteit van de neerslag nochtans noodzakelijk om voordelen ten opzichte van lumped modellen te verkrijgen, namelijk het inzicht in de interne hydrologische processen van het stroomgebied. Interpolatietechnieken zijn een middel om ruimtelijke schattingen te verkrijgen in plaats van puntschattingen. Nochtans zijn discontinuteiten in klimatologische gegevens moeilijk volledig te beschrijven. Vele methoden zijn voorhanden, met ondermeer de algemeen gebruikte Thiessen polygonen, de Inverse Distance Weighting (IDW) en de kriging methoden. In dit werk zijn interpolaties van de neerslag uitgevoerd met Thiessen polygonen, IDW en ordinary kriging. De mogelijkheid om de hoogteligging mee op te nemen werd nagegaan. Vermits geen duidelijk verband tussen de hoogteligging en de neerslag kon worden aangetoond is dit niet verder uitgewerkt. Het semi-ruimtelijk verdeelde model SWAT (Soil and Water Assessment Tool) is toegepast om de invloed van interpolatietechnieken op de modellering van de hydrologie van het Nzoia stroomgebied in Kenia te veri eren. De neerslaginput in SWAT vertoont sterke simpli caties waarbij voor elk deelbekken de neerslag wordt overgenomen van het dichtst bijzijnde station. Hierdoor worden bepaalde stations buiten beschouwing gelaten wat een verlies aan informatie iii Hoofdstuk 0. Samenvatting iv betekent. Een bijkomend probleem is de onvolledigheid van de neerslaggegevens afkomstig van het Metereologisch Instituut van Kenia. Bovenstaande twee problemen worden grotendeels omzeild door het toepassen van de opgestelde interpolatietechnieken. De interpolatietechnieken rekenen dagelijks met alle beschikbare stations, waardoor alle beschikbare informatie wordt gebruikt. Vermits per dag enkel met de beschikbare gegevens wordt gewerkt, is het niet noodzakelijk de ontbrekende data vooraf aan te vullen. De neerslag wordt opnieuw uitgemiddeld per deelbekken zodat de neerslag kan worden toegekend aan de deelbekkens via virtuele stations, gelegen op de centroden van de subbasins. Een initiele autokalibratie werd uitgevoerd met de oorspronkelijke input van de meetstations. In een eerste fase werden de bekomen parameters gebruikt bij de modellering met de gegenereerde neerslaginput, waarbij geen opmerkelijke verbetering in de modelresultaten werd gevonden. In een tweede fase werd een nieuwe autokalibratie uitgevoerd met de gegenereerde neerslaginput, waardoor een e ectieve verbeterde simulatie van de debietmetingen kon worden vastgesteld, wat het potentieel aangeeft van het gebruik van verbeterde invoer van neerslag voor het SWAT model, hoewel het onderlinge onderscheid tussen de technieken beperkt blijft.},
author = {{Van Hoey}, S.},
keywords = {Modelling,interpolation,optimisation},
mendeley-tags = {Modelling,interpolation,optimisation},
pages = {106},
school = {University Ghent},
title = {{Invloed van ruimtelijke neerslaginterpollatietechnieken op de hydrologische modellering van het Nzoia stroomgebied}},
url = {http://studwww.ugent.be/~stvhoey/thesis/Thesis\_stijnvanhoey\_af.pdf},
year = {2008}
}
