year = {1992}
}
@article{Al-Haque2012a,
abstract = {Effective estimation of parameters in biocatalytic reaction kinetic expressions are very important when building process models to enable evaluation of process technology options and alternative biocatalysts. The kinetic models used to describe enzyme-catalyzed reactions generally include several parameters, which are strongly correlated with each other. State-of-the-art methodologies such as nonlinear regression (using progress curves) or graphical analysis (using initial rate data, for example, the Lineweaver-Burke plot, Hanes plot or Dixon plot) often incorporate errors in the estimates and rarely lead to globally optimized parameter values. In this article, a robust methodology to estimate parameters for biocatalytic reaction kinetic expressions is proposed. The methodology determines the parameters in a systematic manner by exploiting the best features of several of the current approaches. The parameter estimation problem is decomposed into five hierarchical steps, where the solution of each of the steps becomes the input for the subsequent step to achieve the final model with the corresponding regressed parameters. The model is further used for validating its performance and determining the correlation of the parameters. The final model with the fitted parameters is able to describe both initial rate and dynamic experiments. Application of the methodology is illustrated with a case study using the $\omega$-transaminase catalyzed synthesis of 1-phenylethylamine from acetophenone and 2-propylamine. © 2012 American Institute of Chemical Engineers Biotechnol. Prog., 2012.},
author = {Al-Haque, Naweed and Santacoloma, Paloma a and Neto, Watson and Tufvesson, P\"{a}r and Gani, Rafiqul and Woodley, John M},
doi = {10.1002/btpr.1588},
file = {::},
issn = {1520-6033},
journal = {Biotechnology progress},
keywords = {biocatalysis,kinetic modeling,omega-transaminases,parameter estimation},
month = jun,
number = {1},
pages = {1--11},
pmid = {22736412},
title = {{A robust methodology for kinetic model parameter estimation for biocatalytic reactions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22736412},
year = {2012}
}
