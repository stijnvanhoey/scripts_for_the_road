isbn = {10.1061/(ASCE)HE.1943-5584.0000128},
journal = {Journal of Hydrologic Engineering},
keywords = {Canada,Discharge,Extension principle,Fuzzy nonlinear regression,Fuzzy number,Fuzzy sets,Measurement,Measurement uncertainty,Rivers,Stage,Stage-discharge relationship,Uncertainty principles,Water discharge},
language = {en},
