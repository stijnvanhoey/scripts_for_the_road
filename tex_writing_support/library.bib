publisher = {Academie des sciences},
title = {{Causal models as multiple working hypotheses about environmental processes}},
url = {http://apps.webofknowledge.com/full\_record.do?product=UA\&search\_mode=CitingArticles\&qid=4\&SID=S2mFqu7N8R1a365YyDl\&page=3\&doc=21\&cacheurlFromRightClick=no http://linkinghub.elsevier.com/retrieve/pii/S1631071312000120},
