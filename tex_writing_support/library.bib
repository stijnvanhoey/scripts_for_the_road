author = {Ion, S. and Marinescu, D. and Cruceanu, S. G. and Iordache, V.},
doi = {10.1016/j.envsoft.2014.09.012},
issn = {13648152},
journal = {Environmental Modelling \& Software},
keywords = {CRS,Catmull-Rom Spline,Cubic polynomials,DEM,DP,DTM,Data Porting,Digital Elevation Model,Digital Terrain Model,ENO,Essentially Non-Oscillating,GIS,Geographic Information System,Hexagonal raster,Hydrological modelling,Interpolation,LiDAR,Light Detection And Ranging,OF,Outlier Filtering,PDEs,Partial Differential Equations,Raster data,Water routing},
month = dec,
pages = {240--252},
title = {{A data porting tool for coupling models with different discretization needs}},
url = {http://www.sciencedirect.com/science/article/pii/S1364815214002667},
volume = {62},
year = {2014}
}
@article{Montanari2013,
abstract = {Observation uncertainty is nowadays recognized as a serious issue undermining the reliability of hydrological studies. For instance, many recent contributions show that river flow observations are affected by errors that may reach 25\% even when state-of-the-art measurement techniques are adopted. Yet, there is still little guidance by the literature on the most appropriate modelling strategies to be adopted under observation uncertainty. We carried out a series of simulation experiments and explored how the selection of appropriate model complexity can help reduce the impact of observation uncertainty. We found that model structure plays a relevant role and, in particular, a description of the relevant physical processes that come into play can effectively contribute to limit the impact of data errors and therefore significantly reduce overall uncertainty. (C) 2012 Elsevier Ltd. All rights reserved.},
annote = {From Duplicate 1 ( },
author = {Montanari, A. and {Di Baldassarre}, G.},
doi = {10.1016/j.advwatres.2012.09.007},
file = {:media/DATA/Artikels/Mendeley/Montanari, Di Baldassarre - 2013 - Advances in Water Resources.pdf:pdf},
issn = {0309-1708},
journal = {Advances in Water Resources},
keywords = {BAYESIAN-ANALYSIS,CALIBRATION,CURVES,DISCHARGE,EVENT,FLOOD FREQUENCY INFERENCE,Hydrology,INFORMATION,INPUT UNCERTAINTY,Model complexity,Monitoring techniques,Observation uncertainty},
month = jan,
pages = {498--504},
