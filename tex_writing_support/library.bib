keywords = {Data standards,Flow forecasting,Hydrological modelling,Model integration,Operational hydrology},
pages = {65--77},
publisher = {Elsevier Ltd},
title = {{The Delft-FEWS flow forecasting system}},
url = {http://dx.doi.org/10.1016/j.envsoft.2012.07.010},
volume = {40},
year = {2013}
}
