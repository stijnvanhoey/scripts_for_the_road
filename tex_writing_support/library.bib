@misc{Vandenberghe2007,
abstract = {Na een eerste zoektocht in de literatuur in het kader van ‘onzekerheid bij hydrologisch modelleren’ bleek reeds dat we te maken hadden met een levendig onderwerp binnen de hydrologie dat een sterke aandacht geniet en zich nog in volle evolutie bevindt. Dit hoeft niet te verbazen aangezien de complexiteit van de onzekerheidsanalyse, door het grote aantal invloeden die erop inspelen, groot is. We hebben immers te maken met “... unknown errors in input and boundary conditions that get processed nonlinearly through a model that has structural errors and which is then compared with observations that have unknown measurement and commensurability error characteristics”. [Beven, 2006] We hebben dan ook getracht in dit projectwerk een overzicht te geven van de belangrijkste modelleringconcepten en definities, gevolgd door een bespreking van de verschillende oorzaken van de onzekerheid, dit vooral toegespitst op neerslag-afvoermodellen. Aangezien er op dit moment tientallen frameworks bestaan voor het inschatten van onzekerheid, waarbij binnen elk framework nog een groot aantal variaties kunnen voorkomen, hebben we hier geenszins getracht een overzicht te geven, laat staan een volledig. We hebben ons hierbij beperkt tot drie voorbeelden, waarvan de GLUE-methode de meest bekende is. Tot slot geven we nog een uiteenzetting over de toekomstperspectieven van onzekerheidsanalyses.},
author = {Vandenberghe, S. and Genetello, E. and {Van Hoey}, S. and Meire, D.},
file = {:home/stvhoey/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Vandenberghe, Genetello, Meire - 2007 - Onzekerheid bij hydrologisch modelleren.pdf:pdf},
keywords = {GLUE,modelling,uncertainty},
mendeley-tags = {GLUE,modelling,uncertainty},
pages = {33},
title = {{Onzekerheid bij hydrologisch modelleren}},
year = {2007}
