keywords = {CALIBRATION,INFERENCE,INPUT UNCERTAINTY,MODELS,MONTE-CARLO},
month = dec,
number = {W12602},
pages = {1--7},
publisher = {AMER GEOPHYSICAL UNION},
title = {{Generalized likelihood uncertainty estimation (GLUE) and approximate Bayesian computation: What's the connection?}},
url = {http://apps.webofknowledge.com/full\_record.do?product=UA\&search\_mode=CitingArticles\&qid=4\&SID=S2mFqu7N8R1a365YyDl\&page=2\&doc=13\&cacheurlFromRightClick=no},
