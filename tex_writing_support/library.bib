publisher = {Taylor \& Francis},
title = {{Kinetic parameters and inhibition response of ammonia- and nitrite-oxidizing bacteria in membrane bioreactors and conventional activated sludge processes}},
url = {http://dx.doi.org/10.1080/09593331003793828},
volume = {31},
